{
  "name": "vocab-trainer-frontend",
  "version": "1.0.0",
  "description": "Browser version of trainer app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/4h-edu/vocab-trainer-frontend.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "postinstall": "npm run download-1 && npm run download-2 && npm run download-3",
    "download-1": "wget https://threejs.org/build/three.min.js -O ./src/www/three.min.js",
    "download-2": "wget https://raw.githubusercontent.com/mrdoob/three.js/master/examples/js/loaders/STLLoader.js -O ./src/www/STLLoader.js",
    "download-3": "wget https://raw.githubusercontent.com/mrdoob/three.js/master/examples/js/controls/OrbitControls.js -O ./src/www/OrbitControls.js"
  },
  "private": true,
  "license": "ISC",
  "homepage": "https://bitbucket.org/4h-edu/vocab-trainer-frontend#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "html-webpack-plugin": "^2.24.1",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "material-ui": "^0.17.1",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-swipeable-views": "^0.11.1",
    "react-swipeable-views-utils": "^0.11.1",
    "react-tap-event-plugin": "^2.0.1",
    "three": "^0.84.0",
    "three-stl-loader": "^1.0.4",
    "three-trackballcontrols": "0.0.5"
  }
}
